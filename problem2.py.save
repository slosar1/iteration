import random

var = 1
while var == 1 :

	number = input("Please enter a number 1 through 1,000:")
	number = int(number)

	secret = random.randint(1,1000+1)
	print(secret)
	if(number > secret):
		print("too high, not in range")
	elif(number < secret):
		print("too low, not in rang")
	else:
		number = input("enter a number again")
	else:
		print("you won!!!")
